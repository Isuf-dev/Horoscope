        android:text="@string/preference2"
        android:textAlignment="center"
        android:textColor="#CCFFFFFF"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/preferenceText8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckedTextView
        android:id="@+id/preferenceText3"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/background_rounded_user_input"
        android:padding="5dp"
        android:text="@string/preference3"
        android:textAlignment="center"
        android:textColor="#CCFFFFFF"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/preferenceText4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckedTextView
        android:id="@+id/preferenceText4"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/background_rounded_user_input"
        android:padding="5dp"
        android:text="@string/preference4"
        android:textAlignment="center"
        android:textColor="#CCFFFFFF"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/preferenceText9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckedTextView
        android:id="@+id/preferenceText5"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:layout_marginTop="13dp"
        android:layout_marginBottom="130dp"
        android:background="@drawable/background_rounded_user_input"
        android:padding="5dp"
        android:text="@string/preference5"
        android:textAlignment="center"
        android:textColor="#CCFFFFFF"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/preferences_next_btn"
        app:layout_constraintVertical_bias="0.981" />

    <CheckedTextView
        android:id="@+id/preferenceText6"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/background_rounded_user_input"
        android:padding="5dp"
        android:text="@string/preference6"
        android:textAlignment="center"
        android:textColor="#CCFFFFFF"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/preferenceText1"
